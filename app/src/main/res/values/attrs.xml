<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GridLayout">
        <attr name="grid_fixRaw" format="integer" />
        <attr name="grid_itemWidth" format="dimension" />
        <attr name="grid_itemHeight" format="dimension|enum" >
            <enum name="auto_height" value="-1"/>
        </attr>
        <attr name="grid_itemHorizontalPadding" format="dimension" />
        <attr name="grid_itemVerticalPadding" format="dimension" />
        <attr name="grid_itemSizeMode" format="enum">
            <enum name="item_width" value="0x00" />
            <enum name="horizontal_padding" value="0x01" />
        </attr>
    </declare-styleable>
</resources>