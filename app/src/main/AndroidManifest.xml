        <activity android:name=".ui.test.GifListActivity" />
        <activity android:name=".ui.gif.GifDetailActivity" />
        <activity android:name=".ui.view.GridLayoutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.view.ByteTextViewActivity" />
        <activity android:name=".ui.gif.GifViewActivity" />
